# Environment Variables for MalricPharma Frontend

# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api/v1
VITE_APP_NAME=MalricPharma
VITE_APP_VERSION=1.0.0

# Development Settings
VITE_ENABLE_DEBUG=true
VITE_LOG_LEVEL=debug

# Payment Configuration (Development)
VITE_MPESA_SANDBOX=true
VITE_MPESA_CONSUMER_KEY=your_sandbox_consumer_key
VITE_MPESA_CONSUMER_SECRET=your_sandbox_consumer_secret

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT_SUPPORT=true
