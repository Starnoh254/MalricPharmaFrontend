# Local Environment Variables (gitignored)
# Copy this file to .env.local and add your personal settings

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api

# Development Settings
VITE_ENABLE_DEBUG=true
VITE_LOG_LEVEL=debug

# Personal API Keys (keep these secret!)
VITE_MPESA_CONSUMER_KEY=your_personal_consumer_key
VITE_MPESA_CONSUMER_SECRET=your_personal_consumer_secret

# Feature Flags for Testing
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_CHAT_SUPPORT=false
